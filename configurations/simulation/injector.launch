<launch> 
    <node name="injector" pkg="injector" type="injector" output="screen" />

    <param name="frequency" value="5" type="int" /> <!--Hz-->

    <param name="components" value="g3t1_2" /> <!--Names of the components in which uncertainty will be injected --> 

    <!--Parameters for g3t1_1 uncertainty injection-->
    <param name="g3t1_1/type" value="step" />                        <!--Could be: step, ramp or random-->
    <param name="g3t1_1/offset" value="0" />
    <param name="g3t1_1/amplitude" value="0.15" />
    <param name="g3t1_1/frequency" value="0.000011574" />            <!-- 1 hour = 1/(24*60*60)-->
    <param name="g3t1_1/duration" value="86400" type="int" />        <!-- 1 hour (s) -->
    <param name="g3t1_1/begin" value="30" type="int" />              <!-- (s) instant to begin the injection-->

    <!--Parameters for g3t1_2 uncertainty injection-->
    <param name="g3t1_2/type" value="ramp" />                        <!--Could be: step, ramp or random-->
    <param name="g3t1_2/offset" value="0" />
    <param name="g3t1_2/amplitude" value="0.15" />
    <param name="g3t1_2/frequency" value="0.000011574" />            <!-- 1 hour = 1/(24*60*60)-->
    <param name="g3t1_2/duration" value="86400" type="int" />        <!-- for long enough input-->
    <param name="g3t1_2/begin" value="30" type="int" />              <!-- (s) instant to begin the injection-->

    <!--Parameters for g3t1_3 uncertainty injection-->
    <param name="g3t1_3/type" value="random" />                      <!--Could be: step, ramp or random-->
    <param name="g3t1_3/offset" value="0" />
    <param name="g3t1_3/amplitude" value="0.15" />
    <param name="g3t1_3/frequency" value="1" />                      <!-- 1 Hz -->
    <param name="g3t1_3/duration" value="0" type="int" />            <!-- not important for random input -->
    <param name="g3t1_3/begin" value="30" type="int" />              <!-- (s) instant to begin the injection-->

    <!--Parameters for g3t1_4 uncertainty injection-->
    <param name="g3t1_4/type" value="step" />                       <!--Could be: step, ramp or random-->
    <param name="g3t1_4/offset" value="0" />
    <param name="g3t1_4/amplitude" value="0.15" />
    <param name="g3t1_4/frequency" value="0.01" />                   <!-- 0.01 Hz-->
    <param name="g3t1_4/duration" value="1" type="int" />            <!-- -->
    <param name="g3t1_4/begin" value="30" type="int" />              <!-- (s) instant to begin the injection-->

</launch>